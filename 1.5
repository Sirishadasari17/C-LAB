#include<stdio.h>
#include<math.h>
int main()
{
	int a,b,c,DISC;
	float root1,root2;
	printf("values of a,b,c");
	scanf("%d%d%d",&a,&b,&c);
	DISC=b*b-4*a*c;
	if(DISC==0)
	{
		printf("Roots are real and equal.\n");
		root1=root2=(-b)/(2*a);
		printf("Root1=%.2f,Root2=%.2f",root1,root2);
	}
	else if(DISC>0)
	{
		printf("Roots are real and Distinct.\n");
		root1=(-b+sqrt(DISC))/(2*a);
		root2=(-b-sqrt(DISC))/(2*a);
		printf("Root1=%.2f,Root2=%.2f",root1,root2);
	}
	else{
		printf("\nRoot are Imaginary");
	}
	return 0;
}//end of main()
