#include<stdio.h>
#include<math.h>
int main()
{
	int n,s=0,r,t,dig=0;
	printf("Enter n value");
	scanf("%d",&n);
	t=n;
	// logic for counting digits
	while(t>0)
	{
		r=t%10; //gets the digit
		dig=dig+1; //count the digits
		t=t/10;
	}
	t=n;
	while(t>0)
	{
		r=t%10;
		s=s+pow(r,dig);
		t=t/10;
	}
	if(n==s) // checking original is same or not
	    printf("%d is ARMSTRONG NUMBER",n);
	else
     	printf("%d is NOT ARMSTRONG NUMBER",n);
	return 0;
	    
}
